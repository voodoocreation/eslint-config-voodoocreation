{
  "name": "eslint-config-voodoocreation",
  "author": "Raice Hannay <voodoocreation@gmail.com>",
  "description": "An opinionated ESLint config with TypeScript support.",
  "license": "ISC",
  "version": "6.0.0",
  "keywords": [
    "typescript",
    "javascript",
    "typescript-eslint",
    "lint",
    "linting",
    "linter",
    "eslint",
    "tslint"
  ],
  "type": "module",
  "main": "index.cjs",
  "files": [
    "index.cjs",
    "base.cjs"
  ],
  "exports": {
    ".": "./index.cjs",
    "./base": "./base.cjs"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "eslint",
    "lint:base": "eslint -c eslint.config.base.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:voodoocreation/eslint-config-voodoocreation.git"
  },
  "bugs": {
    "url": "https://github.com/voodoocreation/eslint-config-voodoocreation/issues"
  },
  "homepage": "https://github.com/voodoocreation/eslint-config-voodoocreation#readme",
  "engines": {
    "node": ">= 8"
  },
  "dependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.8.0",
    "typescript-eslint": "^7.16.1"
  },
  "devDependencies": {
    "eslint": "^9.7.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "eslint": ">= 9.7.0",
    "eslint-plugin-import": ">= 2.29.1",
    "eslint-plugin-jest": ">= 28.6.0",
    "eslint-plugin-jsx-a11y": ">= 6.9.0",
    "eslint-plugin-prefer-arrow": ">= 1.2.3",
    "eslint-plugin-react": ">= 7.34.2",
    "eslint-plugin-react-hooks": ">= 4.6.2",
    "prettier": ">= 3.3.3",
    "typescript": ">= 5.5.3"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-jsx-a11y": {
