{
  "name": "eslint-config-voodoocreation",
  "author": "Raice Hannay <voodoocreation@gmail.com>",
  "description": "An opinionated ESLint config with TypeScript support.",
  "license": "ISC",
  "version": "7.0.1",
  "keywords": [
    "typescript",
    "javascript",
    "typescript-eslint",
    "lint",
    "linting",
    "linter",
    "eslint",
    "tslint"
  ],
  "type": "module",
  "main": "index.cjs",
  "files": [
    "index.cjs",
    "base.cjs"
  ],
  "exports": {
    ".": "./index.cjs",
    "./base": "./base.cjs"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "eslint",
    "lint:base": "eslint -c eslint.config.base.js"
