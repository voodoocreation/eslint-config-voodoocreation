  "repository": {
    "type": "git",
    "url": "git@github.com:voodoocreation/eslint-config-voodoocreation.git"
  },
  "bugs": {
    "url": "https://github.com/voodoocreation/eslint-config-voodoocreation/issues"
  },
  "homepage": "https://github.com/voodoocreation/eslint-config-voodoocreation#readme",
  "engines": {
    "node": ">= 8"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "typescript-eslint": "^8.15.0"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0",
